<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">



    <servlet>
        <servlet-name>bbg</servlet-name>
        <servlet-class>be.intecbrussel.servlet.Login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>bbg</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>bbg2</servlet-name>
        <servlet-class>be.intecbrussel.servlet.Logout</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>bbg2</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>be.intecbrussel.servlet.Register</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Transfer</servlet-name>
        <servlet-class>be.intecbrussel.servlet.Transaction_servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Transfer</servlet-name>
        <url-pattern>/transferMoney</url-pattern>
    </servlet-mapping>

</web-app>